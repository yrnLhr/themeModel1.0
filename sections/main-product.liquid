            {%- when 'tl_comment_carousel' -%}
            <!-- 评论轮播 -->
            {% assign imageArray = block.settings.tl_comment_carousel_imgarr | split: ',' %}
            {% assign tl_imageArray_size = imageArray | size %}
            <div class="tl-slideshow" style="margin: -1rem 0">
              <div class="tl-slides-container" style="width: {{ tl_imageArray_size | times: 100 }}%">
                {% for image in imageArray %}
                  <div class="tl-slide" style="width: {{ 1.00 | divided_by: tl_imageArray_size | times: 100 }}%">
                    <img src="{{ image | asset_url }}" alt="Image">
                  </div>
                {% endfor %}
              </div>
            </div>
            <script>
              var slidesContainer = document.querySelector('.tl-slides-container')
              var slideWidth = document.querySelector('.tl-slide').offsetWidth
              var currentIndex = 0
        
              function slideNext() {
                currentIndex++
                if (currentIndex === 3) {
                  currentIndex = 0
                }
                slidesContainer.style.transform =
                  'translateX(' + -currentIndex * slideWidth + 'px)'
              }
        
              setInterval(slideNext, 2000)
            </script>
            
            {%- when 'price' -%}
              <div class="no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                {%- render 'price',
                  product: product,
                  use_variant: true,
                  show_badges: true,
                  price_class: 'price--large'
                -%}
              </div>
              {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
                {% comment %} <div class="product__tax caption rte">
                  {%- if shop.taxes_included -%}
                    {{ 'products.product.include_taxes' | t }}
                  {%- endif -%}
                  {%- if shop.shipping_policy.body != blank -%}
                    {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- endif -%}
